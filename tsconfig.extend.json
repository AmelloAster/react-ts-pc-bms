{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@views/*": [
        "src/views/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@services/*": [
        "src/shared/services/*"
      ],
      "@config/*": [
        "src/core/config/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@core/*": [
        "src/core/*"
      ]
    }
  }
}
